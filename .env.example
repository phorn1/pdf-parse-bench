# Example environment variables for PDF parsing services
# Copy this file to .env and fill in your actual API keys

# ========== BENCHMARK API KEYS ==========

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# ========== PARSER API KEYS ==========

# LlamaCloud/LlamaParse Configuration
LLAMACLOUD_API_KEY=your_llamacloud_api_key_here

# Mistral Configuration
MISTRAL_API_KEY=your_mistral_api_key_here

# Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# MinerU Configuration
MINERU_API_KEY=your_mineru_api_key_here

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Mathpix Configuration
MATHPIX_APP_KEY=your_mathpix_app_key_here
MATHPIX_APP_ID=your_mathpix_app_id_here
